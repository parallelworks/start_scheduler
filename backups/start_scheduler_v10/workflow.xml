<tool id='gtdemo_start_scheduler_v10' name='gtdemo_start_scheduler_v10'>
  <command interpreter='parsl'>main.py</command>
  <inputs>
    <section name='required_info' type='section' title='Required Info' expanded='true'>
      <param name='executor_pools' type='select' label='Select Executor Pools' width='100%' help='Select executor pools to run GT packets. If more than one pool is selected the smart pool selector will distribute the load to the pools.' multiple='true'>
        <option value="gtexec_2vcpu" selected="true">gtexec_2vcpu</option>
        <option value="gtexec_4vcpu" selected="true">gtexec_4vcpu</option>
        <option value="gtexec_8vcpu" selected="true">gtexec_8vcpu</option>
        <option value="gtexec_16vcpu" selected="true">gtexec_16vcpu</option>
        <option value="gtexec_32vcpu" selected="true">gtexec_32vcpu</option>
        <option value="gtexec_64vcpu" selected="true">gtexec_64vcpu</option>
        <option value="gtexec_96vcpu" selected="true">gtexec_96vcpu</option>
      </param>
      <param name='license' label='License Server' type='text' value='27005@50.232.253.140:27005@50.232.253.141:27005@50.232.253.142' help="License servers: port1@host1:port2@host2. On Windows, the separator is a semicolon." width='100%'>
      </param>
    </section>
    <section name='advanced_info' type='section' conditional='[object Object]' title='Advanced Info' expanded='false'>
      <param name='gt_user' label='GT user name' type='text' value='gtuser' help='GT_USER parameter in gtdistd file' width='50%'>
      </param>
      <conditional name="sum_serv_cond">
        <param name="sum_serv" type="boolean" truevalue="Yes" falsevalue="No" checked="true" label="Enable Summary Service"  help='Summary service is a feature which outputs a summary of job attributes to a csv file upon a job finishing' width="50%" float="right"/>
       </conditional>
    </section>
  </inputs>
  <outputs>
  </outputs>
</tool>
